pico-8 cartridge // http://www.pico-8.com
version 29
__lua__
function _init()
  beer = {}
  beers = 0
  beerctr = 0
  beerx = 52
    
  yoff=0
  x=58
  cx=58
  vel=30
  maxvel=50
  minvel=30
  y=130-(vel)
  add_beer()
end

function add_beer()
  -- find a free beer
  nidx=-1
  for i=1, beers do
    if beer[i].x == 0 then
      idx=i
      break
    end
  end
  
  if nidx < 0 then
     beers += 1
     nidx = beers
  end

  rx = flr(rnd(3))
  if rx == 0 then
    beerx -= 22
  elseif rx == 2 then
    beerx += 22
  end
  
  if beerx < 40 then
    beerx = 40
  elseif beerx > 106 then
    beerx = 106
  end
     
  beer[nidx] = {}
  beer[nidx].x = beerx
  beer[nidx].y = -16
end

function _update()

  beerctr += vel
  if beerctr > 250 then
    beerctr = 0
    add_beer()
  end
  
  yoff+=(vel/10)
  if yoff > 128 then
    yoff=0
  end
  y=130-(vel)
  
  if btnp(⬅️) then
    x = x - 22
    if x < 36 then
      x = 36
    end
  elseif btnp(➡️) then
    x = x + 22
    if x > 102 then
      x = 102
    end
  end
  
  if btn(⬆️) then
    vel += 1
    if vel > maxvel then
      vel = maxvel
    end
  elseif btn(⬇️) then
    vel -= 1
    if vel < minvel then
      vel = minvel
    end
  end
  
  if cx > x then
    cx -= 5.5
  elseif cx < x then
    cx += 5.5
  end
 
  for i=1, beers do
    if beer[i].x > 0 then
       beer[i].y += (vel/10)
       if beer[i].y > 128 then
          beer[i].x = 0
       end
    end
  end
end

function _draw()
  cls()
  istart = ceil(yoff/16)*-16
  for i=istart, 128-istart,16 do
    camera(0,yoff*-1)
    map(0,0, 0,i,16,2)
  end
  palt(0,false)
  camera(0,0)
  
  if cx == x then
    spr(1,cx,y,2,3)
  elseif cx > x then
    spr_r(1,cx,y,-6,2,3)
  else
    spr_r(1,cx,y,6,2,3)
  end
  
  for i=1, beers do
    if beer[i].x > 0 then
      spr(39,beer[i].x,beer[i].y)
    end
  end
  
  print("" .. flr(vel*10),0,0)
  print("" .. cx,0,8)
  print("" .. x,0,16)
end


function spr_r(s,x,y,a,w,h)
 sw=(w or 1)*8
 sh=(h or 1)*8
 sx=(s%8)*8
 sy=flr(s/8)*8
 x0=flr(0.5*sw)
 y0=flr(0.5*sh)
 a=a/360
 sa=sin(a)
 ca=cos(a)
 for ix=0,sw-1 do
  for iy=0,sh-1 do
   dx=ix-x0
   dy=iy-y0
   xx=flr(dx*ca-dy*sa+x0)
   yy=flr(dx*sa+dy*ca+y0)
   if (xx>=0 and xx<sw and yy>=0 and yy<=sh) then
    pset(x+ix,y+iy,sget(sx+xx,sy+yy))
   end
  end
 end
end
