-- explosions

function init_expl()
  expl = {}
  explidx = 0
end

function add_expl(explx,exply)
  -- find a free slot, use it
  local idx = 0
  for i=1, explidx do
    if expl[i].c == 0 then
      idx = i
      break
    end
  end
  
  if idx == 0 then
    explidx += 1
    idx = explidx
  end
  
  sfx(2)
  expl[idx] = {}
  expl[idx].x = explx
  expl[idx].y = exply
  expl[idx].s = 0
  expl[idx].c = 1
end

function update_expl()
  for i=1, explidx do
    if expl[i].c > 0 then
      expl[i].c += 1
      if expl[i].c > 30 then
        expl[i].c = 0
      else
      end
    end
  end
end

function draw_expl_frame(ee)
  --for i=0, 360, flr(rnd(18)) do
  srand(1)
  for i=0, 360, 6 do
    col = flr(rnd(3))+8
    --col = flr(ee.c/2)
        
    cc = (ee.c*rnd(3))/6 + rnd(3)
    xx = cc*cos(i/360)
    yy = cc*sin(i/360)
    if (i%40) > ee.c then
      pset(ee.x+xx, ee.y+yy, col)
    end
    cc = ee.c*rnd(3) + rnd(10)
    xx = cc*cos(i/360)
    yy = cc*sin(i/360)
    if (i%40) > ee.c then
      pset(ee.x+xx, ee.y+yy, col)
    end
  end
  
  --circ(ee.x, ee.y, ee.c, 3)
end

function draw_expl()
  for i=1, explidx do
    if expl[i].c > 0 then
       draw_expl_frame(expl[i])
    end
  end
end
