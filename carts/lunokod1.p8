__lua__
-- main

function _init()

  -- mode 0=attract, 1=game
  mode = 0
  score=0
  guys=3
  maxguys=5
    
  debug=false
  maxp=100
  area=512
  maxx=area-16

  -- f=ma, a=f/m
  -- v=p/m
  dtime=(1/30)
  m=1
  f=600
  ff=150
  dpf=ff*dtime
  dp=f*dtime
  
  y=112
  p=0.0
  v=0.0
  
  scrloff=(area/2)-64
  minscr=0
  maxscr=(area-128)

  winsize=32
  lwinlimit=(128-winsize)/2
  rwinlimit=(area-lwinlimit)
  lwin=scrloff+lwinlimit
  rwin=lwin+winsize
  x=lwin+8

  init_bullets()
  _init_enemies()  
  init_expl()

  sfx(1)  
end

function dist(x1,y1,x2,y2)
  return sqrt((x2-x1)^2 + (y2-y1)^2)
end

function _update()

  if mode == 0 then
    update_attract()
    return
  end

  update_e()     
  --
  -- process inputs
  --
  
  -- enable/disable debug
  if btnp(üÖæÔ∏è) then
     debug=not debug
  end
  
  -- add propulsion force
  if btn(‚¨ÖÔ∏è) then
    p=p-dp
  elseif btn(‚û°Ô∏è) then
    p=p+dp
  end
  
  update_bullets()

  -- apply friction
  if dpf > abs(p) then
    p=0
  elseif p > 0 then
    p=p-dpf
  else
    p=p+dpf
  end
  
  -- adjust velocity / position
  v=(p/m)
  dx=flr(v*dtime)  
  x=x+dx

  -- bound momemtum
  if p < (maxp*-1) then
    p=maxp*-1
  elseif p > maxp then
    p=maxp
  end
    
  -- bound position
  if x > maxx then
    x = maxx
    p=0.0
  elseif x < 0 then
    x = 0
    p=0.0
  end
  
  -- update scroll offset
  if x < lwinlimit then
    scrloff=0
  elseif x > rwinlimit then
    scrloff=area-128
  elseif x < lwin then
    scrloff+=dx
    if scrloff < 0 then
       scrloff=0
    end
  elseif x > rwin then
    scrloff+=dx
    if scrloff > maxscr then
      scrloff=maxscr
    end
  end  
  lwin=scrloff+48
  rwin=lwin+32
   
  update_expl()

  -- collision detection
  for h=1, bidx do
    if b[h][2] > 0 then
      xmn=b[h][1]-4
      xmx=b[h][1]+4
      ymn=b[h][2]-6
      ymx=b[h][2]+6
      for j=1, eidx do
        if e[j].x >= xmn then
          if e[j].x <= xmx then
            if e[j].y >= ymn then
              if e[j].y <= ymx then
                score += 1
                add_expl(e[j].x+4,e[j].y+4)
                b[h][2]=-128
                e[j].y=-128          
              end
            end
          end
        end
      end
    end
  end
  -- rover collision
  for j=1, eidx do
    if e[j].y > 104 then
      if e[j].x > (x-4) then
        if e[j].x < x then 
          if guys > 0 then
            guys -= 1
          end
        end
      end
    end
  end
end

function _draw()
  if mode == 0 then
    draw_attract()
    return
  end

  cls()
  palt(1,true)
  palt(0,false)
  
  camera(scrloff/4)
  srand(1)
  for i=1, 100 do
    pset(rnd(220),rnd(116),rnd(3)+5)
  end
  
  camera(0)
  if debug then
    print("x:" .. x,0,0)
    print("lwin:" .. lwin,0,8)
    print("rwin:" .. rwin,0,16)
    print("scrloff:" .. scrloff,0,24)
    print("p:" .. p, 0, 32)
    print("eidx:" .. eidx, 0, 40)
    print("bidx:" .. bidx, 0, 48) 
  end  
  
  if scrloff > 80 then
    camera(scrloff/3)
    map(0,4,150,88,4,2)
  end

  camera(scrloff/2)  
  if scrloff < 256 then
    map(0,0,0,88,16,4)
  else 
    map(0,0,256,88,16,4)
  end
  map(0,0,128,88,16,4) 

  camera(scrloff)
  spr(3,x,110,2,1)
 
  for i=1, eidx do
    if e[i].y > -128 then
      spr(e[i].s,e[i].x,e[i].y)
    end
  end 
  
  --camera(scrloff)
  if scrloff < 128 then
    map(0,6,0  ,112,16,1)
    map(0,6,128,112,16,1)
  elseif scrloff >= 256 then
    map(0,6,256,112,16,1)
    map(0,6,384,112,16,1)
  else
    map(0,6,128,112,16,1)
    map(0,6,256,112,16,1)
  end
  
  --map(0,3,0-scrloff,120,48,1)
  for i=1, bidx do
    if b[i][2] > 0 then
      spr(2,b[i][1],b[i][2])
    end
  end
  

  
  draw_expl()
   
  camera(0)
  print("" .. score, 1, 121)
  for i=1, guys do
    spr(34,126-(i*8),120)
  end
end
