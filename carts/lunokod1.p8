-- enemies

function _init_enemies()
  e = {}
  eidx = 0
  ectr = 0
  ernd = srand(1)

  -- enemy table
  et = {}

  et[1] = {}
  et[1].xmin=0
  et[1].xrange=area/4
  et[1].xv = 2
  et[1].s = 12
  
  et[2] = {}
  et[2].xmin=0
  et[2].xrange=area/2
  et[2].xv = 1
  et[2].s = 12
  
  et[3] = {}
  et[3].xmin=0
  et[3].xrange=area
  et[3].xv = 0
  et[3].s = 11
  
  et[4] = {}
  et[4].xmin=area/2
  et[4].xrange=area/2
  et[4].xv = -1
  et[4].s = 10
  
  et[5] = {}
  et[5].xmin=area-(area/4)
  et[5].xrange=area/4
  et[5].xv = -2
  et[5].s = 10 
end

function get_e(a)
 
  bn = {}
  bn.x = flr(rnd(et[a].xrange))+et[a].xmin
  bn.y = -8
  bn.xv = rnd(1)*et[a].xv
  bn.yv = rnd(2)
  bn.s = et[a].s
  
  return bn
end

function update_e()

  if ectr == 0 then
    -- find a free slot
    local idx=-1
    for i=1, eidx do
      if e[i].y == -128 then
        idx = i
        break
      end
    end
    
    if idx < 0 then
      eidx += 1
      idx = eidx
    end
    
    srand(ernd)
    ernd = rnd(100)
    
    e[idx] = get_e(flr(rnd(4))+1)
    ectr = 10
  else
    ectr -= 1
  end

  -- update enemy positions
  for i=1, eidx do
    if e[i].y > 112 then
       e[i].y = -128
    elseif e[i].y > -128 then
       e[i].y += e[i].yv
       e[i].x += e[i].xv
    end
  end 
end
