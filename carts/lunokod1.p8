-- bullets

function init_bullets()
  b = {}
  bidx = 0
  bctr = 0  
end

function update_bullets()
  -- launch missiles
  if btn(‚ùé) then
    if bctr > 0 then
      bctr-=1
    else
      bctr=7
      newidx=0
      for i=1, bidx do
        if b[i][2] <= 0 then
          newidx=i
          break
        end
      end
      if newidx == 0 then
        bidx+=1
        newidx=bidx
      end
      sfx(0)
      --add_expl(x,64)
      b[newidx] = {x+3,y-4}
    end
  else
    bctr=0
  end
  
  -- update missile positions
  for i=1, bidx do
    if b[i][2] > 0 then
      b[i][2]-=5
    end
  end
end
