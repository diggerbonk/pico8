function get_e()
 
  a = etmap[flr(rnd(etmapsz))+1]
  
  --if rnd(1) < min(.5,(wave/10)) then
  --  a = flr(rnd(4))+4
  --end
  
  bn = {}
  bn.x = flr(rnd(et[a].xrange))+et[a].xmin
  bn.y = -8
  bn.yv = max(0.8,
    rnd(1.5) + (wave/20))
  maxxv = bn.yv
  bn.xv = rnd(maxxv)*et[a].xv
  bn.ci = et[a].ci
  bn.s = et[a].s
  
  return bn
end

function get_vis_e()
  for i=1, eidx do
    if e[i].y > 0 then
      return true
    end
  end
  return false
end

function update_e()
    
  if mode==1 then
    if ectr == 0 then
      -- find a free slot
      local idx=-1
      for i=1, eidx do
        if e[i].y == -128 then
          idx = i
          break
        end
      end
    
    
      if not epaused then
        if idx < 0 then
          eidx += 1
          idx = eidx
        end
    
        srand(ernd)
        ernd = rnd(100)
    
        e[idx] = get_e()
      end
      
      -- min value 4 max 12
      
      if wave > 8 then
        ectr = 4
      else
        ectr = 12-wave
      end

    else
      ectr -= 1
    end
  end

  -- update enemy positions
  for i=1, eidx do
    if e[i].y > 120 then
       if e[i].ci == 2 then
         rads+=1
         if rads >= maxrads then
           sfx(7,-2)
           rads = maxrads
           mode = 4
           countdown=30
         elseif not radsalarm then
           if rads > (maxrads*.70) then
             radsalarm=true
             sfx(7)
           end
         end
       end
       e[i].y = -128
    elseif e[i].y > -128 then
       e[i].y += e[i].yv
       e[i].x += e[i].xv
    end
  end 
end
