-- main

function _init()

  -- mode 0=attract, 1=game
  mode = 0
  score=0
  guys=3
  maxguys=5
  countdown=0
  wave=1
  wavectr=0
  wavectrmax=200
  animcounter=0
  framecounter=0
  animod=0
  rads=0
  maxrads=20
  radsalarm=false
    
  debug=false
  maxp=100
  area=256
  maxx=area-16

  -- f=ma, a=f/m
  -- v=p/m
  dtime=(1/30)
  m=1
  f=600
  ff=150
  dpf=ff*dtime
  dp=f*dtime
  
  y=120
  p=0.0
  v=0.0
  
  scrloff=(area/2)-64
  minscr=0
  maxscr=(area-128)

  winsize=32
  lwinlimit=(128-winsize)/2
  rwinlimit=(area-lwinlimit)
  lwin=scrloff+lwinlimit
  rwin=lwin+winsize
  x=lwin+8

  init_bullets()
  _init_enemies()  
  init_expl()

  starfield = {}
  for i=1, 100 do
    starfield[i] = {}
    starfield[i][1] = flr(rnd(220))
    starfield[i][2] = flr(rnd(116))
    starfield[i][3] = flr(rnd(3))+5
  end
  
  sfx(1)
  --sfx(7)
  music(0)  
end

function dist(x1,y1,x2,y2)
  return sqrt((x2-x1)^2 + (y2-y1)^2)
end
  
function _update()

  framecounter+=1
  animcounter += 1
  if animcounter > 3 then
    animcounter=0
    animod+=16
    if animod > 48 then
      animod = 0
    end
  end
  
  if countdown > 0 then
    countdown -= 1
  elseif get_vis_e() == false then
    if mode == 0 then
      update_attract()
      return
    elseif mode == 2 then
      mode = 3
      countdown = 60
      sfx(3)
    elseif mode == 3 then
      epaused=false
      wavectr=0 
      mode = 1
    elseif mode == 4 then
      mode = 0
      radsalarm=false
      sfx(7,-2)
      sfx(0,-2)
      music(1)
    elseif mode == 1 and wavectr > 200 then
      wavectr = 0
      countdown=20
      epaused = false
      mode = 2
    end
  else
    if wavectr == 200 then
      wavectr += 1
      if mode == 1 then
        wave +=1
        epaused = true
      end
    elseif wavectr < 200 then
      wavectr += 1
    end
  end
    
  

  update_e()     
  --
  -- process inputs
  --
  
  -- enable/disable debug
  if btnp(ðŸ…¾ï¸) then
     debug=not debug
  end
  
  -- add propulsion force
  if mode==1 then
    if btn(â¬…ï¸) then
      p=p-dp
    elseif btn(âž¡ï¸) then
      p=p+dp
    end
  end
  
  update_bullets()

  -- apply friction
  if dpf > abs(p) then
    p=0
  elseif p > 0 then
    p=p-dpf
  else
    p=p+dpf
  end
  
  -- adjust velocity / position
  v=(p/m)
  dx=flr(v*dtime)  
  x=x+dx

  -- bound momemtum
  if p < (maxp*-1) then
    p=maxp*-1
  elseif p > maxp then
    p=maxp
  end
    
  -- bound position
  if x > maxx then
    x = maxx
    p=0.0
  elseif x < 0 then
    x = 0
    p=0.0
  end
  
  -- update scroll offset
  if x < lwinlimit then
    scrloff=0
  elseif x > rwinlimit then
    scrloff=area-128
  elseif x < lwin then
    scrloff+=dx
    if scrloff < 0 then
       scrloff=0
    end
  elseif x > rwin then
    scrloff+=dx
    if scrloff > maxscr then
      scrloff=maxscr
    end
  end  
  lwin=scrloff+48
  rwin=lwin+32
   
  update_expl()

  -- collision detection
  for h=1, bidx do
    if b[h][2] > 0 then
      xmn=b[h][1]-4
      xmx=b[h][1]+4
      ymn=b[h][2]-6
      ymx=b[h][2]+6
      for j=1, eidx do
        if e[j].x >= xmn then
          if e[j].x <= xmx then
            if e[j].y >= ymn then
              if e[j].y <= ymx then
                score += 1
                add_expl(e[j].x+4,e[j].y+4,e[j].ci)
                b[h][2]=-128
                e[j].y=-128          
              end
            end
          end
        end
      end
    end
  end

  -- rover collision
  if mode==1 then
    for j=1, eidx do
      if e[j].y > 112 then
        if e[j].x > (x-4) then
          if e[j].x < (x+8) then
            mode = 2
            countdown = 30
            add_expl(x+8,y+4,e[j].ci)
            --add_expl(x,y)
            --add_expl(x+4,y+8)
            e[j].y = -128
            guys -= 1
            if guys == 0 then
              countdown=30
              mode=4
            end 
          end
        end
      end
    end
  end
end

function _draw()

  cls()
  palt(1,true)
  palt(0,false)

  -- starfield
  camera(scrloff/4)
  for i=1, 100 do
    pset(starfield[i][1],
      starfield[i][2],
      starfield[i][3])
  end
  
  -- attract mode
  if mode == 0 then
    draw_attract()
    --return
  end
  
  -- earth
  camera(scrloff/3)
  map(0,4,area/2,90,4,4)

  -- remote hills
  camera(scrloff/2)  
  if scrloff < 256 then
    map(0,0,0,96,16,4)
  else 
    map(0,0,256,96,16,4)
  end
  map(0,0,128,96,16,4) 

  -- draw lunokhod
  camera(scrloff)
  
  if mode == 1 then
    spr(3+flr(x%16/4)*16,x,118,2,1)
  elseif mode==3 then
    if (countdown % 3) == 0 then
      spr(3+animod,x,118,2,1)
    end
  end
 
  -- draw enemies
  for i=1, eidx do
    if e[i].y > -128 then
      spr(e[i].s+animod,e[i].x,e[i].y)
    end
  end 
  
  -- draw bottom terrain
  if scrloff < 128 then
    map(0,9,0  ,120,16,1)
    map(0,9,128,120,16,1)
  elseif scrloff >= 256 then
    map(0,9,256,120,16,1)
    map(0,9,384,120,16,1)
  else
    map(0,9,128,120,16,1)
    map(0,9,256,120,16,1)
  end
  
  -- bullets
  for i=1, bidx do
    if b[i][2] > 0 then
      spr(0+animod,b[i][1],b[i][2])
    end
  end
  
  -- exlosions
  draw_expl()
  
  -- status 
  camera(0)
  
  if mode==3 then
    print("wave " .. wave, 4,4,framecounter%15+1)
  elseif mode==1 then
    print("wave " .. wave, 4,4, 6)
  end
  
  --radiation
  rect(52,10,77,13,0)
  rectfill(54,12,76,13,5)
  rectfill(53,11,75,12,6)
  if rads > 0 then
    rectfill(53,11,53+rads,12,framecounter%16)
  end
  
  -- free guys
  for i=1, guys do
    spr(50,126-(i*8),2)
  end
  
  -- print score
  len = #("" .. score)
  for i=0, (5-len) do
    print("0", 54+i*4, 4, 5)
    print("0", 53+i*4, 3, 7)
  end
  print("" .. score, 78-(len*4), 4, 5)
  print("" .. score, 77-(len*4), 3, 7)

  if debug then
    print("x:" .. x,0,0)
    print("lwin:" .. lwin,0,8)
    print("rwin:" .. rwin,0,16)
    print("scrloff:" .. scrloff,0,24)
    print("p:" .. p, 0, 32)
    print("eidx:" .. eidx, 0, 40)
    print("bidx:" .. bidx, 0, 48) 
    print("mode:" .. mode, 0, 56)
    print("countdown:" .. countdown, 0, 64)
    print("wavectr:" .. wavectr, 65, 0)
  
    print("wave:" .. wave, 65,8)                
    print("epaused:" .. tostring(epaused), 65, 16)
    print("ectr:" .. ectr, 65, 24)
  end 
end
